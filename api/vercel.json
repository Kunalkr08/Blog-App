{
    "version": 2,
    "builds": [
        {
            "src": "*.js",
            "use": "@vercel/node",
        }
    ],
    "routes": [
        {
            "src": "/",
            "dest": "/api/index.js",
            "methods": ["GET", "POST", "PUT", "PATCH", "DELETE", "OPTIONS"]
        }
    ]
}
